<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hibernate.model">
	<class name="User" table="users">
		<id name="id" type="int" column="id" />

		<property name="userName" column="user_name" type="java.lang.String" />
		<property name="name" column="name" type="java.lang.String" />
		<property name="plainPassword" column="plain_password" type="java.lang.String" />
		<property name="encryptedPassword" column="encrypted_password" 	type="java.lang.String" />
		<property name="tell" column="tell" type="java.lang.String" />
		<property name="email" column="email" type="java.lang.String" />
		<property name="createDate" column="create_date" type="java.util.Date" />
		<property name="updateDate" column="update_date" type="java.util.Date" />

	</class>
</hibernate-mapping>